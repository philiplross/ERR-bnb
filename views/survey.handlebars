<div id="survey">
  <div class="container">
    <div class="row">
      <h5>Questions</h5>
      <div class="col-8 offset-2">
        <form class="form-group">
        {{#each questions}}
        
          <p>  {{this.question}} <div class="form-check form-check-inline">
  <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio1" value="option1">
  <label class="form-check-label" for="inlineRadio1">Blank1</label>
<input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option2">
  <label class="form-check-label" for="inlineRadio1">Blank2</label>
  <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio3" value="option3">
  <label class="form-check-label" for="inlineRadio1">Blank3</label>

</div></p>




         
        {{/each}}
       </form>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>